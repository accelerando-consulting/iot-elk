[ -n "$ELASTICSEARCH" ] || ELASTICSEARCH=hostnamegoeshere
[ -n "$ELASTICUSER" ]   || ELASTICUSER=elastic
[ -n "$ELASTICPASS" ]   || ELASTICPASS=passwordgoeshere
[ -n "$ELASTICPROTO" ]  || ELASTICPROTO=https
[ -n "$ELASTICPORT" ]   || ELASTICPORT=9243
[ -n "$ELASTICURL" ]    || ELASTICURL=${ELASTICPROTO}://${ELASTICSEARCH}:${ELASTICPORT}
export ELASTICURL ELASTICUSER ELASTICPASS
